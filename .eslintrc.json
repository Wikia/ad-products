{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "class-methods-use-this": 0,
        "comma-dangle": 0,
        "func-names": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "import/prefer-default-export": 0,
        "indent": [2, "tab", {"VariableDeclarator": 1, "SwitchCase": 1}],
        "max-len": [2, 120, 2],
        "no-mixed-operators": 0,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "no-tabs": 0,
        "one-var": 0,
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "__useDefault"
                ]
            }
        ]
    },
    "globals": {
        "document": false,
        "window": false,
        "top": false
    }
}
