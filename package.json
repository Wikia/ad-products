{
  "name": "@wikia/ad-products",
  "version": "4.2.1",
  "description": "Wikia ad products",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wikia/ad-products"
  },
  "dependencies": {
    "@wikia/ad-engine": "^9.7.0",
    "core-decorators": "^0.20.0",
    "events": "^1.1.1",
    "lodash": "^4.17.4",
    "regenerator-runtime": "^0.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-provide-modules": "1.0.3",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "mocha-jsdom": "^1.1.0",
    "mocha-junit-reporter": "^1.15.0",
    "mocha-webpack": "^1.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.3.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "4.1.1"
  },
  "files": [
    "dist/",
    "src/",
    "jsconfig.json",
    "README.md"
  ],
  "main": "./dist/ad-products.js",
  "publishConfig": {
    "registry": "https://artifactory.wikia-inc.com/artifactory/api/npm/wikia-npm/"
  },
  "scripts": {
    "build": "./node_modules/webpack/bin/webpack.js --progress --colors --hide-modules",
    "lint": "./node_modules/.bin/eslint .",
    "prepublishOnly": "./maintenance/prepublish.sh",
    "preversion": "./maintenance/preversion.sh",
    "version": "./maintenance/version.sh",
    "serve": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --watch --inline --open-page=templates/ --content-base=./examples",
    "test": "mocha-webpack './spec/**/*.spec.js' -r jsdom-global/register --webpack-env.test",
    "test-coverage": "nyc mocha-webpack './spec/**/*.spec.js' -r jsdom-global/register --webpack-env.test --reporter mocha-junit-reporter --reporter-options mochaFile=./spec/build/tests.xml",
    "watch": "./node_modules/webpack/bin/webpack.js --progress --colors --hide-modules --watch"
  },
  "nyc": {
    "report-dir": "./spec/build/",
    "reporter": [
      "cobertura"
    ]
  }
}
